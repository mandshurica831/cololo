{
  "name": "cololo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=lib node ./bin/www",
    "stop": "pkill myApp",
    "watch-css": "nodemon -e ./src/scss/*.scss --ignore public/ -x \"npm run css\"",
    "css": "node-sass --output-style compact src/scss/style.scss public/css/style.css --importer ./node_modules/node-sass-globbing/index.js",
    "watch-js": "nodemon -e ./src/js/*.js --ignore public/ -x \"npm run gulp\"",
    "gulp": "gulp",
    "webpack": "webpack",
    "build": "webpack & node-sass --include-path scss scss/style.scss css/style.css"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "colors": "^1.1.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.4.1",
    "ejs-locals": "^1.0.2",
    "express": "~4.13.4",
    "express-session": "^1.14.0",
    "jquery": "^3.1.1",
    "moment": "^2.14.1",
    "morgan": "~1.7.0",
    "multer": "^1.2.0",
    "mysql": "^2.11.1",
    "q": "^1.4.1",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.8",
    "glob": "^7.1.1"
  },
  "devDependencies": {
    "css-loader": "^0.25.0",
    "db-migrate": "^0.10.0-beta.15",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp-concat": "^2.6.1",
    "node-sass": "^3.13.1",
    "nodemon": "^1.11.0",
    "sass-glob-importer": "^1.0.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  }
}
